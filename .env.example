# Runix Environment Configuration

# Engine Settings
RUNIX_DRIVER=exampledriver
RUNIX_PARALLEL_SCENARIOS=false
RUNIX_REPORT_PATH=reports/runix-report.json

# Logging Settings
RUNIX_LOG_LEVEL=INFO
RUNIX_LOG_FILE=logs/runix-engine.log
RUNIX_LOG_CONSOLE=true
RUNIX_LOG_TRACING=true
RUNIX_LOG_TIMESTAMP_FORMAT=ISO

# Driver Settings
RUNIX_DRIVER_DIR=./drivers
RUNIX_DEFAULT_DRIVER=WebDriver
RUNIX_DRIVER_TIMEOUT=30000

