# Runix Environment Configuration

# Engine Settings
RUNIX_DRIVER=exampledriver
RUNIX_PARALLEL_SCENARIOS=false
RUNIX_REPORT_PATH=reports/runix-report.json

# Logging Settings
RUNIX_LOG_LEVEL=INFO
RUNIX_LOG_FILE=logs/runix-engine.log
RUNIX_LOG_CONSOLE=true
RUNIX_LOG_TRACING=true
RUNIX_LOG_TIMESTAMP_FORMAT=ISO

# Driver Settings
RUNIX_DRIVER_DIR=./drivers
RUNIX_DEFAULT_DRIVER=WebDriver
RUNIX_DRIVER_TIMEOUT=30000
RUNIX_DRIVER_DEBUG=true

# WebDriver Specific Settings
RUNIX_WEB_BROWSER=chrome
RUNIX_WEB_HEADLESS=false
RUNIX_WEB_SLOWMO=100
RUNIX_WEB_VIEWPORT_WIDTH=1280
RUNIX_WEB_VIEWPORT_HEIGHT=720
RUNIX_WEB_TIMEOUT=30000

# API Driver Settings
RUNIX_API_BASE_URL=http://localhost:3000
RUNIX_API_TIMEOUT=5000
RUNIX_API_RETRY=3

# Example Driver Settings
RUNIX_EXAMPLE_DELAY=500

# Feature Settings
RUNIX_FEATURE_DIR=./scenarios
RUNIX_DEFAULT_TAGS=@smoke,@dev

# AI Driver Settings - Multiple Provider Support
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_DEPLOYMENT=your_azure_deployment_name_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# AI Model Configuration
AI_DEFAULT_MODEL=gpt-4o-mini
AI_COMPUTER_USE_MODEL=gpt-4o-with-canvas
AI_VISION_MODEL=gpt-4o-mini
AI_AGENT_MODEL=gpt-4o-with-canvas
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=4096

# Computer Use Configuration
ENABLE_COMPUTER_USE=true
COMPUTER_USE_PROVIDER=openai
COMPUTER_USE_RESOLUTION=1280x720
COMPUTER_USE_QUALITY=low

# Database Settings
# RUNIX_DB_TYPE=postgres
# RUNIX_DB_HOST=localhost
# RUNIX_DB_PORT=5432
# RUNIX_DB_USERNAME=user
# RUNIX_DB_PASSWORD=password
# RUNIX_DB_NAME=runix
