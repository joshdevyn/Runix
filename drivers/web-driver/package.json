{
  "name": "runix-unified-web-driver",
  "version": "2.0.0",
  "description": "Professional unified web driver for Runix automation engine supporting both Playwright and Selenium WebDriver",
  "main": "index.js",  "scripts": {
    "start": "node index.js",
    "build": "node build.js",
    "dev": "node index.js --port=8000",
    "test": "node index.js --port=8000"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "selenium-webdriver": "^4.15.0",
    "ws": "^8.18.2"
  },  "devDependencies": {
    "pkg": "^5.8.1"
  },  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": ".",
    "assets": [
      "driver.json",
      "selenium-server.jar"
    ],
    "scripts": [
      "lib/**/*.js"
    ],
    "patches": {
      "playwright-core": [
        "lib/server/chromium/appIcon.png"
      ]
    }
  },
  "author": "Runix Team",
  "license": "MIT"
}
