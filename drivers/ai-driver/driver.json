{
  "id": "ai-driver",
  "name": "AIDriver",
  "description": "Enhanced AI Driver with Multi-Provider Support - Integrates OpenAI, Anthropic, Ollama, and Azure OpenAI for advanced AI capabilities",
  "version": "3.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "index.js",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection",
    "ai-coordination",
    "workflow-generation",
    "vision-analysis",
    "session-management",
    "multi-mode-operation"
  ],  "actions": [
    "generateFeature",
    "analyzeIntent",
    "discoverDrivers",
    "generateSteps",
    "orchestrate",
    "setMode",
    "startSession",
    "analyzeScreenAndPlan",
    "executeNextAction",
    "loadFeatureFile",
    "continueSession",
    "callAI",
    "enhanceWithAI",
    "generateAnswer",
    "switchProvider",
    "addProvider",
    "getProviders",
    "getActiveProvider"
  ],
  "modes": {
    "agent": "AI takes control and executes tasks autonomously",
    "ask": "AI works with user to complete specific tasks with confirmation",
    "chat": "AI provides guidance and explains what actions would be taken"
  },  "config": {
    "model": "gpt-4-vision-preview",
    "temperature": 0.7,
    "maxTokens": 4000,
    "defaultMode": "chat",
    "providers": [
      {
        "name": "OpenAI",
        "type": "openai",
        "apiKey": "${OPENAI_API_KEY}",
        "model": "gpt-4o"
      }
    ],
    "activeProvider": "OpenAI"
  }
}
