{
  "id": "ai-driver",
  "name": "AIDriver",
  "description": "AI orchestration driver for intent analysis and workflow generation",
  "version": "1.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "index.js",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection",
    "health-check",
    "ai-coordination"
  ],
  "actions": [
    "generateFeature",
    "analyzeIntent",
    "discoverDrivers",
    "generateSteps",
    "orchestrate"
  ],
  "config": {
    "openaiApiKey": "",
    "model": "gpt-4",
    "temperature": 0.7,
    "maxTokens": 2000
  }
}
