{
  "id": "vision-driver",
  "name": "VisionDriver",
  "description": "Computer vision driver for OCR and UI element detection with GPT-4o support",
  "version": "1.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "VisionDriver.exe",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection",
    "health-check",
    "openai-vision",
    "computer-use-analysis"
  ],
  "actions": [
    "extractText",
    "detectUI",
    "findImage",
    "analyzeScene",
    "analyzeForComputerUse"
  ],
  "config": {
    "ocrLanguage": "eng",
    "confidenceThreshold": 0.6,
    "tempDir": "./temp",
    "providers": {
      "primary": "tesseract",
      "fallback": "openai"
    },
    "openai": {
      "enabled": true,
      "model": "gpt-4o",
      "maxTokens": 2000,
      "temperature": 0.1
    }
  }
}