{
  "id": "ai-driver",
  "name": "AIDriver",
  "description": "AI orchestration driver for autonomous task completion",
  "version": "1.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "AIDriver.exe",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection"
  ],
  "actions": [
    "agent",
    "editor",
    "ask",
    "analyze",
    "plan",
    "execute",
    "confirm",
    "learn",
    "generate",
    "observe",
    "interact"
  ],
  "config": {
    "model": "computer-use-preview",
    "temperature": 0.7,
    "maxTokens": 2000,
    "confirmActions": true,
    "outputDir": "./ai-artifacts",
    "visionDriver": "VisionDriver",
    "systemDriver": "SystemDriver"
  }
}