{
  "id": "web-driver",
  "name": "Unified WebDriver",
  "description": "Professional unified web browser automation driver supporting both Playwright and Selenium WebDriver",
  "version": "2.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "WebDriver.exe",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection",
    "health-check",
    "multi-engine"
  ],  "actions": [
    "open",
    "goto",
    "goBack",
    "goForward",
    "reload",
    "refresh",
    "click",
    "doubleClick",
    "rightClick",
    "hover",
    "focus",
    "blur",
    "type",
    "enterText",
    "clear",
    "pressKey",
    "pressKeys",
    "getText",
    "getInnerText",
    "getInnerHTML",
    "getAttribute",
    "getProperty",
    "getValue",
    "getCSSProperty",
    "isVisible",
    "isHidden",
    "isEnabled",
    "isDisabled",
    "isChecked",
    "isEditable",
    "assertVisible",
    "assertText",
    "verifyText",
    "selectOption",
    "check",
    "uncheck",
    "uploadFile",
    "screenshot",
    "takeScreenshot",
    "screenshotElement",
    "waitForElement",
    "waitForElementVisible",
    "waitForElementHidden",
    "waitForText",
    "waitForUrl",
    "waitForLoadState",
    "getTitle",
    "getUrl",
    "getPageSource",
    "executeJS",
    "executeAsyncJS",
    "switchToFrame",
    "switchToMainFrame",
    "getFrameCount",
    "getFrameNames",
    "queryInShadowDOM",
    "clickInShadowDOM",
    "typeInShadowDOM",
    "setViewportSize",
    "getViewportSize",
    "maximizeWindow",
    "addCookie",
    "getCookies",
    "deleteCookie",
    "deleteAllCookies",
    "setLocalStorage",
    "getLocalStorage",
    "removeLocalStorage",
    "clearLocalStorage",    "setSessionStorage",
    "getSessionStorage",
    "removeSessionStorage",
    "clearSessionStorage",
    "dragAndDrop",
    "tap",
    "swipe",
    "acceptAlert",
    "dismissAlert",
    "getAlertText",
    "interceptRequest",
    "blockUrls",
    "introspect"
  ],
  "config": {
    "engine": "playwright",
    "browserType": "chromium",
    "headless": false,
    "timeout": 30000,
    "screenshotDir": "screenshots",
    "windowSize": { "width": 1280, "height": 720 },
    "retries": 3,
    "retryDelay": 2000
  },
  "engines": {
    "playwright": {
      "browsers": ["chromium", "firefox", "webkit"]
    },
    "selenium": {
      "browsers": ["chrome", "firefox", "edge", "safari"]
    }
  }
}