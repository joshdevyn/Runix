{
  "id": "web-driver",
  "name": "WebDriver",
  "description": "Playwright-based web browser automation driver",
  "version": "1.0.0",
  "author": "Runix Team",
  "license": "MIT",
  "executable": "WebDriver.exe",
  "protocol": "websocket",
  "transport": "websocket",
  "features": [
    "execute",
    "introspection",
    "health-check"
  ],
  "actions": [
    "open",
    "click",
    "enterText",
    "getText",
    "waitForElement",
    "screenshot",
    "assertVisible",
    "assertText",
    "getTitle",
    "getUrl"
  ],
  "config": {
    "browserType": "chromium",
    "headless": false,
    "timeout": 15000,
    "screenshotDir": "screenshots"
  }
}